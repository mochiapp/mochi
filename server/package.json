{
  "name": "mochiserver",
  "description": "MochiServer",
  "version": "0.0.2",
  "license": "MIT",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Jonathan Frazier",
    "email": "jjfraz11@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^9.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "dev": "node_modules/nodemon/bin/nodemon.js ./src/index.js",
    "test": "yarn run eslint && yarn run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit",
    "restart": "pm2 startOrRestart ../ecosystem.config.js",
    "deploy:production": "pm2 deploy ../ecosystem.config.js production",
    "deploy:staging": "pm2 deploy ../ecosystem.config.js staging"
  },
  "dependencies": {
    "@feathersjs/configuration": "1.0.2",
    "@feathersjs/errors": "3.3.0",
    "@feathersjs/express": "1.2.3",
    "@feathersjs/feathers": "3.1.6",
    "@feathersjs/socketio": "3.2.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "feathers-blob": "2.0.1",
    "fs-blob-store": "5.2.1",
    "gun": "^0.9.98",
    "helmet": "3.12.1",
    "ipfs": "^0.28.2",
    "serve-favicon": "2.5.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "mocha": "5.2.0",
    "nodemon": "^1.17.2",
    "pm2": "2.10.4",
    "request": "2.87.0",
    "request-promise": "4.2.2"
  }
}
